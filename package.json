  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "yarn run update-scripts && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "launch": "yarn build && surge ./public hova-labs.surge.sh",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "serve": "gatsby serve",
    "start": "yarn update-scripts && yarn run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit",
    "update-blog-manifest": "node ./scripts/updateBlogManifest.js && eslint ./src/sitePages/blog/Blog/blogManifest.ts --fix",
    "update-route-list": "node ./scripts/updateRoutesList.js && eslint ./src/routes.ts --fix",
    "update-scripts": "yarn update-route-list && yarn update-blog-manifest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
